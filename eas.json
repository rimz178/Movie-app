{
  "cli": {
    "version": ">= 16.3.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_NO_PREBUILD_MODIFY": "1"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "env": {
          "EXPO_NO_PREBUILD_MODIFY": "1"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
